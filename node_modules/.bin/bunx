#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rafaeltab/source/message_system/node_modules/.pnpm/bun@1.0.1/node_modules/bun/bin/node_modules:/home/rafaeltab/source/message_system/node_modules/.pnpm/bun@1.0.1/node_modules/bun/node_modules:/home/rafaeltab/source/message_system/node_modules/.pnpm/bun@1.0.1/node_modules:/home/rafaeltab/source/message_system/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rafaeltab/source/message_system/node_modules/.pnpm/bun@1.0.1/node_modules/bun/bin/node_modules:/home/rafaeltab/source/message_system/node_modules/.pnpm/bun@1.0.1/node_modules/bun/node_modules:/home/rafaeltab/source/message_system/node_modules/.pnpm/bun@1.0.1/node_modules:/home/rafaeltab/source/message_system/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../bun/bin/bun"   "$@"
exit $?
